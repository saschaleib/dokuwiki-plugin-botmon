		 "bot": 30
		},
		{"func": "combinationTest", "params": [["macos", "msie"], ["win10", "safari"]],
		 "id": "impPC", "desc": "Impossible combination of platform and client",
		 "bot": 100
		},
		{"func": "loadSpeed", "params": [3, 20],
