		{"func": "fromKnownBotIP",
		 "id": "botIpRange", "desc": "Common Bot IP range",
		 "bot": 50
		},
		{"func": "matchesClient", "params": ["aol", "msie", "chromeold","oldedge"],
		 "id": "oldClient", "desc": "Obsolete browser version",
